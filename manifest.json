{
    "name": "Switcher",
    "version": "0.1",
    "description": "Tool to easily switch between tabs.",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "Launch Switcher"
        }
    },
    "background": {
        "scripts": ["switcher.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}